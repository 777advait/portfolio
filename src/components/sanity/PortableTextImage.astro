---
import { urlForImage } from "@/sanity/lib/urlForImage";
import { Image } from "astro:assets";

const { asset } = Astro.props.node;
const webpUrl = urlForImage(asset).quality(75).format("webp").url();
---

<Image
  src={webpUrl}
  alt="image"
  loading="lazy"
  decoding="async"
  width={800}
  height={600}
/>
